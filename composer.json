{
    "name"       : "automattic/nginx-http-concat",
    "description": "WP.com plugin to perform CSS and Javascript concatenation of individual scripts into a single script.",
    "homepage"   : "https://github.com/Automattic/nginx-http-concat",
    "type"       : "wordpress-plugin",
    "license"    : "GPL-2.0+",
    "require"    : {
        "php": ">=7.4",
        "composer/installers": "^1.0 || ^2.0"
    },
    "require-dev": {
        "yoast/phpunit-polyfills": "^1.1.0"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
